@model TMNT.Models.IntermediateStandard

@{
    ViewBag.Title = "Details";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            @Model.IdCode Intermediate Standard Details
        </h1>
        <ol class="breadcrumb">
            <li class="active">
                <i class="fa fa-dashboard"></i>@Html.ActionLink(" Dashboard", "Index", "Home") / <i class="fa fa-balance-scale"></i> Intermediate Standards / <i class="fa fa-clipboard"></i> @Model.IdCode Intermediate Standard Details
            </li>
        </ol>
    </div>
</div>

<p>
    @Html.ActionLink("Back to List", "Index", "IntermediateStandard", htmlAttributes: new { @class = "btn btn-default" })
</p>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.DateCreated)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DateCreated)
        </dd>

        @if (Model.Replaces == null) {
            <dt>
                Replaces
            </dt>

            <dd>
                N/A
            </dd>
        } else {
            <dt>
                @Html.DisplayNameFor(model => model.Replaces)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Replaces)
            </dd>
        }

        @if (Model.Replaces == null) {
            <dt>
                Replaced By
            </dt>

            <dd>
                N/A
            </dd>
        } else {
            <dt>
                @Html.DisplayNameFor(model => model.ReplacedBy)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ReplacedBy)
            </dd>
        }

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.IntermediateStandardId })
    @*@Html.ActionLink("Back to List", "Index")*@
</p>
