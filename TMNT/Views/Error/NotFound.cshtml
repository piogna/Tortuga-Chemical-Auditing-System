
@{
    Layout = "";
    ViewBag.Title = "Not Found";
}
<style>

    #four-oh-four {
        -webkit-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
        -moz-transform: rotate(45deg);
        -o-transform: rotate(45deg);
        transform: rotate(45deg);
        font-size: 18em !important;
        margin-bottom:60px;
    }

    @@media screen and (min-width:650px) {
        #four-oh-four {
            -webkit-transform: none;
            -ms-transform: none;
            -moz-transform: none;
            -o-transform: none;
            transform: none;
            font-size: 26em !important;
            margin-bottom: 0;
        }
    }

    p {
        font-size:1.5em !important;
    }

    .list-title {
        font-size:2.2em;
        font-weight:bold;
    }

    li {
        font-size:1.4em;
        display:inline;
    }

    .sm-margin-top {
        margin-top:20px;
    }

    #site-map-title {
        font-size:2.5em;
    }

    .error-top {
        color:#428bca;
    }

    li a {
        color: #fff;
        border:1px solid white;
        padding:5px 40px 5px 40px;
    }

    li a:hover {
        color: #428bca;
        background:#fff;
    }

    li a:active {
        color: #fff;
        background:#953B65;
    }

    #wrapper {
        background:#428bca;
        height:100%;
        color:#fff;
        text-align:center;
    }

    @@media screen and (min-width: 992px) {
        body {
            overflow-y: hidden;
        }

        #wrapper {
            text-align: initial;
        }

        .list-title {
            font-size: 1.5em;
            font-weight: bold;
        }

        li {
            font-size: 1.1em;
            display: list-item;
        }

            li a {
                color: #fff;
                border: none;
                padding-left: 10px;
            }

                li a:hover {
                    color: #953B65;
                    background:none;
                }

                li a:active {
                    color: #953B65;
                    background: none;
                }
    }

</style>
